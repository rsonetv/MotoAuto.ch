    "build": "next build",
    "dev": "node server.cjs",
    "dev:next": "next dev",
    "lint": "next lint",
    "start": "cross-env NODE_ENV=production node server.cjs",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:basic": "jest tests/basic --testTimeout=10000",
    "test:integration": "jest tests/integration --testTimeout=30000",
    "test:docker": "docker-compose -f docker-compose.test.yml up --build --abort-on-container-exit",
    "test:docker:clean": "docker-compose -f docker-compose.test.yml down -v",
    "test:setup": "node scripts/setup-test-env.js",
    "test:db:setup": "psql -h localhost -p 5433 -U postgres -d motoauto_test -f scripts/setup-test-database.sql",
    "test:db:clean": "psql -h localhost -p 5433 -U postgres -d motoauto_test -c 'DROP SCHEMA IF EXISTS test CASCADE;'",
    "docker:test:up": "docker-compose -f docker-compose.test.yml up -d",
    "docker:test:down": "docker-compose -f docker-compose.test.yml down",
    "docker:test:logs": "docker-compose -f docker-compose.test.yml logs -f"
  },
